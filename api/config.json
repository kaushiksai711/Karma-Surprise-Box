{
  "reward_width": 40,
  "reward_probability_threshold": 0.6,
  "reward_rules": {
    "streak_engager": {
      "conditions": [
        "login_streak >= 3",
        "posts_created >= 1",
        "quizzes_completed >= 1"
      ],
      "description": "Consistent engagement across multiple days"
    },
    "quiz_enthusiast": {
      "conditions": [
        "quizzes_completed >= 2",
        "login_streak >= 2"
      ],
      "description": "Regular quiz participation"
    },
    "community_champion": {
      "conditions": [
        "posts_created >= 2",
        "upvotes_received >= 12",
        "buddies_messaged >= 2"
      ],
      "description": "High-quality content creator and community engager"
    },
    "knowledge_contributor": {
      "conditions": [
        "quizzes_completed >= 1",
        "karma_earned_today >= 25"
      ],
      "description": "Active learner and knowledge sharer"
    },
    "social_butterfly": {
      "conditions": [
        "buddies_messaged >= 5",
        "(posts_created >= 1 or comments_written >= 2)"
      ],
      "description": "Active in social interactions"
    },
    "balanced_contributor": {
      "conditions": [
        "posts_created >= 2",
        "comments_written >= 2",
        "buddies_messaged >= 2",
        "karma_earned_today >= 15"
      ],
      "description": "Well-rounded community participation"
    },
    "karma_trader": {
      "conditions": [
        "karma_spent >= 15",
        "karma_earned_today >= 20"
      ],
      "description": "Active in both giving and receiving karma"
    },
    "rising_star": {
      "conditions": [
        "upvotes_received >= 6",
        "karma_earned_today >= 12",
        "login_streak <= 5"
      ],
      "description": "New user showing great potential"
    },
    "creative_scholar": {
      "conditions": [
        "posts_created >= 1",
        "quizzes_completed >= 1",
        "upvotes_received >= 3"
      ],
      "description": "Combining creativity with knowledge"
    },
    "community_glue": {
      "conditions": [
        "buddies_messaged >= 4",
        "karma_spent >= 8",
        "login_streak >= 2"
      ],
      "description": "Holding the community together"
    },
    "active_supporter": {
      "conditions": [
        "login_streak >= 3",
        "karma_earned_today >= 8"
      ],
      "description": "Consistent and supportive community member"
    },
    "mystery_enthusiast": {
      "conditions": [
        "quizzes_completed >= 1",
        "(posts_created >= 1 or comments_written >= 2)"
      ],
      "description": "Enthusiastic participant in various activities"
    },
    "quiz_completion": {
      "conditions": [
        "quizzes_completed > 0"
      ],
      "description": "Completed a quiz"
    }
  },
